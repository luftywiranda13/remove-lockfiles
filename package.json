{
  "name": "remove-lockfiles",
  "version": "0.0.0-development",
  "description": "Prevent committing lockfiles",
  "license": "MIT",
  "author": {
    "name": "Lufty Wiranda",
    "email": "lufty.wiranda@gmail.com",
    "url": "https://www.instagram.com/luftywiranda13/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/luftywiranda13/remove-lockfiles.git"
  },
  "engines": {
    "node": ">=4",
    "npm": ">=3"
  },
  "scripts": {
    "start": "nps --no-scripts",
    "commitmsg": "opt --in commit-msg --exec \"validate-commit-msg\"",
    "precommit": "node cli.js && lint-staged && opt --in pre-commit --exec \"nps test\"",
    "test": "nps test"
  },
  "bin": {
    "remove-lockfiles": "cli.js"
  },
  "files": [
    "cli.js",
    "index.js"
  ],
  "dependencies": {
    "shelljs": "0.7.8"
  },
  "devDependencies": {
    "all-contributors-cli": "4.4.0",
    "commitizen": "2.9.6",
    "cz-conventional-changelog": "2.0.0",
    "eslint": "4.4.1",
    "eslint-config-airbnb-base": "11.3.1",
    "eslint-plugin-import": "2.7.0",
    "husky": "0.14.3",
    "jest": "20.0.4",
    "lint-staged": "4.0.2",
    "nps": "5.7.0",
    "nps-utils": "1.3.0",
    "opt-cli": "1.5.1",
    "prettier": "1.5.3",
    "validate-commit-msg": "2.14.0"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    }
  },
  "lint-staged": {
    "src/**/*.js": [
      "prettier --trailing-comma es5 --single-quote --write",
      "eslint --fix",
      "git add"
    ]
  },
  "keywords": [
    "üê∞"
  ]
}
